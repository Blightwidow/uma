{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@app/*": [
        "app/*"
      ],
      "@lib/*": [
        "lib/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@web-worker/*": [
        "web-worker/*"
      ]
    },
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "pretty": true
  },
  "ts-node": {
    "experimentalSpecifierResolution": "node"
  },
  "extends": "expo/tsconfig.base",
  "exclude": [
    "node_modules"
  ],
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
